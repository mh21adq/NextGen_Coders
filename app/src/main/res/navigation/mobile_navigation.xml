<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.next.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_examples"
        android:name="com.example.next.ui.examples.ExamplesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_examples" />

    <fragment
        android:id="@+id/navigation_compiler"
        android:name="com.example.next.ui.compiler.CompilerFragment"
        android:label="@string/compiler"
        tools:layout="@layout/fragment_compiler" />

    <fragment
        android:id="@+id/navigation_chat_bot"
        android:name="com.example.next.ui.ai.AiAssistantFragment"
        android:label="@string/AI_assistant"
        tools:layout="@layout/fragment_chat_bot" />
    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.next.ui.user.UserFragment"
        android:label="@string/user"
        tools:layout="@layout/fragment_user" />
</navigation>